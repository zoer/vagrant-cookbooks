{
  "run_list": ["recipe[main]"],
  "elasticsearch": {
    "version": "1.2.2",
    "allocated_memory": "512m"
  },
  "rbenv": {
    "group_users": ["vagrant","root"]
  },
  "java": {
    "install_flavor": "openjdk"
  },
  "redisio": {
    "default_settings": {
      "loglevel": "notice"
    }
  },
  "mongodb": {
    "install_method": "mongodb-org",
    "package_version": "2.6.3"
  }
}